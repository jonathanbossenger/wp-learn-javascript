{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "title": "WP Learn: Javascript Block",
  "name": "wp-learn-javascript/javascript-block",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": "Hello JavaScript Block"
    },
    "backgroundColor": {
      "type": "string",
      "default": "pale-pink"
    }
  },
  "supports": {
    "align": true,
    "color": true,
    "spacing": {
      "margin": true
    },
    "className": false
  },
  "category": "layout",
  "icon": "media-code",
  "editorScript": "file:./block.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}